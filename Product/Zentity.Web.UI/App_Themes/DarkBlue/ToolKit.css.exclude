.rStyle
{	
	font-size: 12px;
	padding-left:2px;
	padding-right:5px;
	padding-bottom:2px;
	padding-top:2px;
	border-style:solid;
	border-color:#bbbbbb;
	border-width:1px;
	vertical-align: top;
}

.rStyle td
{
	padding-left:2px;
	padding-right:5px;
	padding-bottom:2px;
	padding-top:2px;
	border-style:solid;
	border-color:#bbbbbb;
	border-width:1px;
	vertical-align: top;
}

.rStyle a:visited
{
    text-decoration:underline;
}

.rStyle a:hover
{
    text-decoration:none;
}

.alternateRowStyle
{
	background-color:#EDF3FE;
	padding-left:2px;
	padding-right:5px;
	padding-bottom:1px;
	padding-top:1px;
	vertical-align: top;
}

.arStyle
