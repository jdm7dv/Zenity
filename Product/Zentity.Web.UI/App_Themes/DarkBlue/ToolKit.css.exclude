.ContextStyle
{
    display: none;
    position: absolute;
    padding: 0px;
    width: 90px;
    height: 80px;
    background-color: White;
    border: solid 1px black;
}
.Invisible
{
    display: none;
}

.Visible
{
    display: inline;
}

.HighLightButton
{
    background-color: #AAC7E5;
}
.DefaultButton
{
    background-color: #FFFFFF;
}
.TreeViewStyle
{
    overflow: auto;
