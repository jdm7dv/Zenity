									</RadialGradientBrush>
								</Ellipse.Fill>
							</Ellipse>
							<TextBlock TextWrapping="Wrap" Text="x" d:LayoutOverrides="Width, Height" Foreground="White" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="13.333" Margin="0,0,0,2" />
						</Grid>
					</ControlTemplate>
				</Setter.Value>
			</Setter>
		</Style>
