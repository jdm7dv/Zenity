										<GradientStop Color="Black" Offset="1" />
									</LinearGradientBrush>
								</Ellipse.Stroke>
								<Ellipse.Fill>
									<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
										<GradientStop Color="#FF45494D" Offset="0" />
										<GradientStop Color="Black" Offset="1" />
									</LinearGradientBrush>
								</Ellipse.Fill>
							</Ellipse>
