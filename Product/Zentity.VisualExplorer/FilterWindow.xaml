			<Border BorderBrush="#FFD5DDE0" BorderThickness="1" Grid.Row="1" CornerRadius="5" Padding="5">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto" MinHeight="24" />
						<RowDefinition />
					</Grid.RowDefinitions>
					<StackPanel Orientation="Vertical" d:LayoutOverrides="Height">
						<TextBlock TextWrapping="Wrap" Text="Entities" FontWeight="Bold" FontSize="13.333" />
						<StackPanel Margin="0,5,0,0">
							<CheckBox Content="All Entities" Margin="1,0,0,0" x:Name="cbAllEntities" IsThreeState="True" ClickMode="Release" />
							<Path Data="M6.5,16 L210,16" Fill="#FFF4F4F5" Height="1" Stretch="Fill" Stroke="#FFD5DDE0" UseLayoutRounding="False" Margin="0,2,0,0" />
						</StackPanel>
					</StackPanel>
					<ListBox ItemContainerStyle="{StaticResource EntityItemStyle}" Grid.Row="1" BorderThickness="0" x:Name="entityListBox" SelectionMode="Extended"></ListBox>
				</Grid>
			</Border>
			<Border BorderBrush="#FFD5DDE0" BorderThickness="1" Grid.Row="1" CornerRadius="5" Padding="5" Grid.Column="2">
				<Grid>
					<Grid.RowDefinitions>
						<RowDefinition Height="Auto" MinHeight="24" />
						<RowDefinition />
					</Grid.RowDefinitions>
					<StackPanel Orientation="Vertical" d:LayoutOverrides="Height">
						<TextBlock TextWrapping="Wrap" Text="Relationships" FontWeight="Bold" FontSize="13.333" />
						<StackPanel Margin="0,5,0,0">
							<CheckBox Content="All Relationships" Margin="1,0,0,0" x:Name="cbAllRelations" IsThreeState="True" ClickMode="Release" />
							<Path Data="M6.5,16 L210,16" Fill="#FFF4F4F5" Height="1" Stretch="Fill" Stroke="#FFD5DDE0" UseLayoutRounding="False" Margin="0,2,0,0" />
						</StackPanel>
					</StackPanel>
					<ListBox ItemContainerStyle="{StaticResource RelationshipItemStyle}" Grid.Row="1" BorderThickness="0" x:Name="relationsListBox" SelectionMode="Extended"></ListBox>
				</Grid>
			</Border>
			<toolkit:BusyIndicator x:Name="busyIndicator" IsBusy="False" BusyContent="Loading Options..." Width="180" Height="60" Margin="198,84,198,102" Grid.Row="1" Grid.ColumnSpan="3" />
		</Grid>
		<StackPanel HorizontalAlignment="Right" Height="30" Orientation="Horizontal" VerticalAlignment="Bottom" Margin="0,0,10,10" Grid.Row="1">
			<Button x:Name="OKButton" Content="OK" Width="90" Margin="3,0" />
			<Button x:Name="CancelButton" Content="Cancel" Width="90" Margin="3,0" />
		</StackPanel>
	</Grid>
